<!--Archivements-->
<div class="about" id="about">
	<div class="container">
		<div class="about-main">
			<div class="about-top wow fadeInDown" data-wow-delay="0.2s">
				<h2>Sobre el colegio</h2>
				<span class="heading-line"> </span>
				<p>{{school.longDescription}}</p>
			</div>
			<div class="about-bottom">
				<div class="col-md-6 about-left wow fadeInLeft" data-wow-delay="0.2s">
					<h4>Ultimos Logros</h4>
					<p>Estamos orgullosos de hasta donde hemos llegado</p>
					{{#school.archivements}}
					<div class="about-grid">
						<div class="about-icon">
							<a href="{{link}}" class="#"> <span class={{type}}> </span> </a>
						</div>
						<div class="about-text">
							<h5>{{name}}</h5>
							<p>{{description}}</p>
						</div>
					   <div class="clearfix"> </div>
					</div>
					{{/school.archivements}}
				</div>

				<div class="col-md-6 about-right wow fadeInRight" data-wow-delay="0.2s">
					<img src="../images/ab.jpg" class="img-responsive" alt="">
				</div>
			   <div class="clearfix"> </div>
			</div>
		</div>
	</div>
</div>
