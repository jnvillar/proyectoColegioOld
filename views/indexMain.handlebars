<!--Contact-->
<div class="contact" id="contact">
	<div class="container">
		<div class="contact-main">
			<div class="contact-top wow fadeInDown" data-wow-delay="0.2s">
				<h3>Contactanos!</h3>
				<span class="heading-line"> </span>

			</div>
			<div class="contact-bottom">
				<div class="col-md-6 contact-left wow fadeInLeft" data-wow-delay="0.2s">
					<form action="/mandarEmail" method="post">
						<input type="text"  value="Tu nombre" onfocus="this.value = '';" onblur="if (this.value == '') {this.value = 'Tu nombre';}">
						<input type="text"  value="Tu email" onfocus="this.value = '';" onblur="if (this.value == '') {this.value = 'Tu email';}">
						<textarea onfocus="this.value = '';" onblur="if (this.value == '') {this.value = 'Mensaje';}">Mensaje</textarea>
						<input type="submit"  value="Send Message">
					</form>
				</div>
				<div class="col-md-6 contact-right wow fadeInRight" data-wow-delay="0.2s">
					<h4>Informacion de Contacto</h4>
					<ul>
					<li><span class="glyphicon glyphicon-map-marker" aria-hidden="true"> </span>{{school.street}}</li>
					<li><span class="glyphicon glyphicon-phone" aria-hidden="true"> </span>{{school.telNumber}}</li>
					<li><span class="glyphicon glyphicon-envelope" aria-hidden="true"> </span><a href="{{school.mail}}">{{school.mail}}</a></li>
				</ul>

				</div>
				 <div class="clearfix"> </div>
			</div>
		</div>
	</div>
</div>
<!--Contact-->

