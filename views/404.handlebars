<!DOCTYPE HTML>

<html>
<head>
    <title>{{page.tittle}}</title>
    <link href="../css/bootstrapMain.css" rel="stylesheet" type="text/css" media="all">
    <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
    <script src="js/jquery-1.11.0.min.js"></script>

    <!-- Custom Theme files -->
    <link href="../css/styleMain.css" rel="stylesheet" type="text/css" media="all"/>
    <!-- Custom Theme files -->

    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="keywords" content="" />
    <script type="application/x-javascript"> addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false); function hideURLbar(){ window.scrollTo(0,1); } </script>

    <!--Google Fonts-->
    <link href='//fonts.googleapis.com/css?family=Hind:400,300' rel='stylesheet' type='text/css'>
    <link href='//fonts.googleapis.com/css?family=Oswald:400,700,300' rel='stylesheet' type='text/css'>
    <!--google fonts-->

    <!-- start-smoth-scrolling -->
    <script type="text/javascript" src="js/move-top.js"></script>
    <script type="text/javascript" src="js/easing.js"></script>
    <script type="text/javascript">
        jQuery(document).ready(function($) {
            $(".scroll").click(function(event){
                event.preventDefault();
                $('html,body').animate({scrollTop:$(this.hash).offset().top},1000);
            });
        });
    </script>
    <!-- //end-smoth-scrolling -->

    <!-- animated-css -->
    <link href="../css/animate.css" rel="stylesheet" type="text/css" media="all">
    <script src="js/wow.min.js"></script>
    <script> new WOW().init(); </script>
    <!-- animated-css -->

    <script src="js/bootstrap.min.js"></script>
    <link href="../css/galleryeffect.css" rel="stylesheet" type="text/css" media="all" />

</head>
<body>
<!--Title-->
<div class="banner" id="home">
    <div class="container">
        <div class="banner-main wow bounceInDown" data-wow-delay="0.2s">
            <div class="fontTitulo">{{page.title}}</div>
            <span class="bann-line"> </span>
            <div class="descriptionMainPage">{{school.description}}</div>
            <a href="#about" class="scroll"><span class="mover"> </span></a>
        </div>
    </div>
</div>
<!--Title-->

<!--Header-->
<div class="header">
    <div class="fixed-header">
        <div class="navbar-wrapper">
            <div class="container">
                <nav class="navbar navbar-inverse navbar-static-top">
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
                            <span class="sr-only">Toggle navigation</span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>
                        <div class="logo wow slideInLeft" data-wow-delay="0.2s">
                            <a class="navbar-brand" href="../"><img src="../images/logo-2.png" /></a>
                        </div>
                    </div>
                    <div id="navbar" class="navbar-collapse collapse">
                        <nav class="cl-effect-1">
                            <ul class="nav navbar-nav">
                                <li><a href="../" class="active scroll">Home</a></li>
                                <li><a href="../courses">Cursos</a></li>
                                <li><a href="#contact" class="scroll">Contact</a></li>
                            </ul>
                        </nav>
                        <!-- script-for sticky-nav -->
                        <script>
                            $(document).ready(function() {
                                var navoffeset=$(".header").offset().top;
                                $(window).scroll(function(){
                                    var scrollpos=$(window).scrollTop();
                                    if(scrollpos >=navoffeset){
                                        $(".header").addClass("fixed");
                                    }else{
                                        $(".header").removeClass("fixed");
                                    }
                                });

                            });
                        </script>
                        <!-- /script-for sticky-nav -->

                    </div>
                    <div class="clearfix"> </div>
                </nav>
            </div>
            <div class="clearfix"> </div>
        </div>
    </div>
</div>
<!--Header-->
<center>
<img align="middle" src="../images/404.jpg" />
</center>
<!--Contact-->
<div class="contact" id="contact">
    <div class="container">
        <div class="contact-main">
            <div class="contact-top wow fadeInDown" data-wow-delay="0.2s">
                <h3>Contactanos!</h3>
                <span class="heading-line"> </span>

            </div>
            <div class="contact-bottom">
                <div class="col-md-6 contact-left wow fadeInLeft" data-wow-delay="0.2s">
                    <form action="/mandarEmail" method="post">
                        <input type="text"  value="Tu nombre" onfocus="this.value = '';" onblur="if (this.value == '') {this.value = 'Tu nombre';}">
                        <input type="text"  value="Tu email" onfocus="this.value = '';" onblur="if (this.value == '') {this.value = 'Tu email';}">
                        <textarea onfocus="this.value = '';" onblur="if (this.value == '') {this.value = 'Mensaje';}">Mensaje</textarea>
                        <input type="submit"  value="Send Message">
                    </form>
                </div>
                <div class="col-md-6 contact-right wow fadeInRight" data-wow-delay="0.2s">
                    <h4>Informacion de Contacto</h4>
                    <ul>
                        <li><span class="glyphicon glyphicon-map-marker" aria-hidden="true"> </span>{{school.street}}</li>
                        <li><span class="glyphicon glyphicon-phone" aria-hidden="true"> </span>{{school.telNumber}}</li>
                        <li><span class="glyphicon glyphicon-envelope" aria-hidden="true"> </span><a href="{{school.mail}}">{{school.mail}}</a></li>
                    </ul>

                </div>
                <div class="clearfix"> </div>
            </div>
        </div>
    </div>
</div>
<!--Contact-->

<!--Maps-->
<div class="map">
    <iframe src="{{school.location}}" frameborder="0" style="border:0" allowfullscreen> </iframe>
</div>
<!--Maps-->

<!--Footer-->
<div class="footer">
    <div class="container">
        <div class="footer-main">
            <div class="col-md-4 ftr-grid wow zoomIn" data-wow-delay="0.2s">
                <h3>Otra Informacion</h3>
                <span class="ftr-line"> </span>
                <p>{{school.telNumber}}</p>
            </div>
            <div class="col-md-4 ftr-grid ftr-mid wow zoomIn" data-wow-delay="0.2s">
                <h3>Redes Sociales</h3>
                <span class="ftr-line flm"> </span>
                <ul >
                    <li><a href="{{school.facebook}}"><span class="fa"> </span></a></li>
                    <li><a href="{{school.twitter}}"><span class="tw"> </span></a></li>
                    <li><a href="{{school.mail}}"><span class="gmail"> </span></a></li>
                </ul>
            </div>
            <div class="col-md-4 ftr-grid ftr-rit wow zoomIn" data-wow-delay="0.2s">
                <h3>Dirección</h3>
                <span class="ftr-line flr"> </span>
                <p>{{school.street}}</p>
                <p> {{school.neighborhood}}</p>
            </div>
            <div class="clearfix"> </div>
        </div>
    </div>
</div>
<!--Footer-->

<!--Copyrights-->
<div class="copy-right">
    <div class="container">
        <div class="copy-rights-main wow zoomIn" data-wow-delay="0.2s">
            <p>© 2016 <a href="{{page.businessPage}}" target="_blank">{{page.businessName}}</a> </p>
        </div>
    </div>
    <script type="text/javascript">
        $(document).ready(function() {
            var defaults = {
                containerID: 'toTop', // fading element id
                containerHoverID: 'toTopHover', // fading element hover id
                scrollSpeed: 1200,
                easingType: 'linear'
            };
            $().UItoTop({ easingType: 'easeOutQuart' });
        });
    </script>
    <a href="#" id="toTop" style="display: block;"> <span id="toTopHover" style="opacity: 1;"> </span></a>

</div>
<!--Copyrights-->

</body>
</html>
